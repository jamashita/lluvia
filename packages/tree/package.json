  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && concurrently \"npm:build:*\"",
    "build:cjs": "tsc -b tsconfig.cjs.json",
