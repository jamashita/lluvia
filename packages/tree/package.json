    "@jamashita/lluvia-sequence": "^2.3.0"
  },
  "devDependencies": {
    "sinon": "11.1.2"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  }
