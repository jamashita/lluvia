    "clean": "concurrently \"npm:clean:*\"",
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "clean:others": "rimraf *.log && rimraf tsconfig.build.tsbuildinfo"
  },
  "dependencies": {
    "@jamashita/anden-error": "2.1.1",
